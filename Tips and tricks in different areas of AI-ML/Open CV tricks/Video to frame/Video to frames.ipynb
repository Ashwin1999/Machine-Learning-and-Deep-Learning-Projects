   "source": [
    "import cv2\n",
    "import time\n",
    "\n",
    "cap = cv2.VideoCapture('original.mp4')\n",
    "\n",
    "i, n = 1, 30\n",
    "\n",
    "while cap.isOpened():\n",
    "    ret, frame = cap.read()\n",
    "    if ret == False:\n",
    "        break\n",
    "        \n",
    "    if i%n==0:\n",
    "        cv2.imwrite('./frames/frame'+str(i/n)+'.jpg',frame)\n",
    "        i+=1\n",
    "    else:\n",
    "        i+=1"
   ]
